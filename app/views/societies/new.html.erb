% @page_title = "Creazione nuovo circolo" %>

<h1>New society</h1>

<% form_for(@society) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :society %><br />
    <%= f.text_field :society %>
  </p>
  <p>
    <%= f.label :base %><br />
    <%= f.text_area :base %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :tel %><br />
    <%= f.text_field :tel %>
  </p>
  <p>
    <%= f.label :opening_time %><br />
    <%= f.text_field :opening_time %>
  </p>
  <p>
    <%= f.label :photo %><br />
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', societies_path %>
